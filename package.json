{
	"name": "filtered-recent-projects",
	"displayName": "Filtered Recent Projects",
	"description": "Show filtered recent projects in VSCode.",
	"version": "0.0.1",
	"engines": {
			"vscode": "^1.58.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/BigFoxMedia/vscode-sibling-spaces"
	},
	"categories": ["Other"],
	"activationEvents": [
			"onCommand:filteredRecentProjects.showFilteredProjects"
	],
	"main": "./extension.js",
	"contributes": {
			"commands": [
					{
							"command": "filteredRecentProjects.showFilteredProjects",
							"title": "Show Filtered Recent Projects"
					}
			]
	},
	"scripts": {
			"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
			"vscode": "^1.1.37",
			"eslint": "^7.32.0",
			"eslint-plugin-vscode": "^0.1.0"
	}
}
